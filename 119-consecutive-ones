public Integer maxConsecutiveOnes(Integer[] numbers) {
    //code here
    integer onesCount=0;
    integer zeroesCount=0;
    for (integer i=0;i<=numbers.size();i++)
    {
        if(numbers[i]==1)
        {
            zeroesCount++;
            if(onesCount>zeroesCount>)
            {zeroesCount=onesCount;}
        }
        else if (numbers[i]==0)
        {
         onesCount++;   
        }
    }
    return zeroesCount;
}