public Integer maxConsecutiveOnes(Integer[] numbers) {
    //code here
    t=0; 

    for(Integer i=0;i<numbers.size();i++){

        if(numbers[i]==1){

            count++;

            if(count>cMax){

                cMax=count;

            }

        }

        else if(numbers[i]==0){

            count=0;

        }

    }

    return cMax;


}