public void setAccountRating(Account a) {

    //code here

    if(a.AnnualRevenue>100,000)
    {
       a.set(Rating,'Cold');
    }
    else if(a.AnnualRevenue<500,000 && a.AnnualRevenue>100,000)
    {
         a.set(Rating,'Warm');
    }
    else if(a.AnnualRevenue>500,000)
    {
        a.set(Rating,'Hot');
    }
    
}