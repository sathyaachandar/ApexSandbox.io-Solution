public void setAccountRating(Account a) {

    //code here

    if (a.AnnualRevenue <= 100,000)
    {
       a.Rating='Cold';
    }
    elseif (a.AnnualRevenue <= 500,000 && a.AnnualRevenue > 100,000)
    {
        a.Rating='Warm';
    }
    elseif (a.AnnualRevenue > 500,000)
    {
        a.Rating='Hot';
    }
    else {
         a.Rating=null;    }
}